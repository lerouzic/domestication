   <text:p text:style-name="P15"><text:a xlink:type="simple" xlink:href="mailto:lerouzic@egce.cnrs-gif.fr" office:target-frame-name="_top" xlink:show="replace" text:style-name="Internet_20_link" text:visited-style-name="Visited_20_Internet_20_Link"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T40">lerouzic@egce.cnrs-gif.fr</text:span></text:span></text:a></text:p>
