   <text:p text:style-name="P36"/>
   <text:p text:style-name="P3">Gene network simulations provide testable predictions for the molecular domestication syndrome</text:p>
   <text:p text:style-name="P3"/>
   <text:p text:style-name="P3"/>
   <text:p text:style-name="P5"><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T1">Ewen Burban</text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T2">*</text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T1">, Maud I Tenaillon</text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T3"></text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T1">, Arnaud Le Rouzic</text:span></text:span><text:span text:style-name="Default_20_Paragraph_20_Font"><text:span text:style-name="T2">*</text:span></text:span></text:p>
